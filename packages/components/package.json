{
  "name": "@monorepo/components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/components/components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook --node-resolve --watch --open --preserve-symlinks",
    "storybook:build": "build-storybook",
    "build.json": "wca analyze \"src/**/*.ts\" --outFile .storybook/custom-elements.json"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^1.15.2",
    "@stencil/core": "^1.12.2",
    "lit-html": "^1.2.1",
    "web-component-analyzer": "^1.0.3"
  },
  "license": "MIT",
  "dependencies": {}
}
